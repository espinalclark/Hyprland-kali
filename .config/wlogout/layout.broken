entry {
    label = "";
    action = "hyprlock";
    text = "Bloquear";
    keybind = "l";
}

entry {
    label = "";
    action = "hyprctl dispatch exit 0";
    text = "Cerrar sesión";
    keybind = "e";
}

entry {
    label = "";
    action = "systemctl reboot";
    text = "Reiniciar";
    keybind = "r";
}

entry {
    label = "";
    action = "systemctl poweroff";
    text = "Apagar";
    keybind = "p";
}

